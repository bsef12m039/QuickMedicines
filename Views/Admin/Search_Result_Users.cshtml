@model List<QMProject.Models.ParentClass>
@{
    ViewBag.Title = "Search_Result_Users";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
<div class="span12" align="center">
    <table class="table">
        <tr>
            <td><p style="color: mediumblue">Image</p></td>
            <td><p style="color: mediumblue">Name</p></td>
            <td><p style="color: mediumblue">User Name</p></td>
            <td><p style="color: mediumblue">Password</p></td>
            <td><p style="color: mediumblue">Category</p></td>
            <td><p style="color: mediumblue">Status</p></td>
            <td><p style="color: mediumblue">House</p></td>
            <td><p style="color: mediumblue">Street</p></td>
            <td><p style="color: mediumblue">Town/Village</p></td>
            <td><p style="color: mediumblue">City</p></td>
            <td><p style="color: mediumblue">State</p></td>
            <td><p style="color: mediumblue">Country</p></td>
            <td><p style="color: mediumblue">Email</p></td>
            <td><p style="color: mediumblue">Cell</p></td>
        </tr>
        @foreach (var s in Model)
        {
            <tr>
                <td><img src=@s.usr.image_add></td>
                <td><p>@s.usr.Name</p></td>
                <td><p>@s.usr.user_name</p></td>
                <td><p>@s.usr.password</p></td>
                <td><p>@s.usr.category</p></td>
                <td><p>@s.usr.status</p></td>
                <td><p>@s.usr.house</p></td>
                <td><p>@s.usr.street</p></td>
                <td><p>@s.usr.town</p></td>
                <td><p>@s.usr.city</p></td>
                <td><p>@s.usr.state</p></td>
                <td><p>@s.usr.country</p></td>
                <td><p>@s.usr.email</p></td>
                <td><p>@s.usr.cell</p></td>
                <td><input type="button" class="btn btn-danger pull-right" id="cart" name="cart" value="Update" /></td>
                <td><input type="button" class="btn btn-danger pull-right" id="cart" name="cart" value="Delete" /></td>
            </tr>
        }
    </table>
    <input type="button" value="Add New" class="btn btn-success" />
</div>